@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Title = "TDS Master";
}

<div class="page-container">
    <!-- Add/Edit form modal -->
    <div id="panelTDSMasterEdit" class="modal fade" data-backdrop="static" data-keyboard="false">
        <div class="modal-dialog modal-md">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title">TDS Master</h5>
                </div>
                <div class="modal-body">
                    <form class="form-horizontal" id="frmTDSCodeMaster" method="get">
                        <div class="form-group">
                            <label class="control-label col-sm-4">TDS Code<span style="color:red"> *</span></label>
                            <div class="col-sm-8">
                                <input type="text" tabindex="1" placeholder="Enter TDS Code" maxlength="50" name="txtCode" id="txtCode" class="form-control required">
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="control-label col-sm-4">TDS Percentage<span style="color:red"> *</span></label>
                            <div class="col-sm-8">
                                <input type="text" tabindex="2" placeholder="Enter TDS Percentage" name="txtPercentage" id="txtPercentage" class="decimal form-control required">
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="control-label col-sm-4">TDS Description<span style="color:red"> *</span></label>
                            <div class="col-sm-8">
                                <input type="text" tabindex="3" placeholder="Enter TDS Description" maxlength="50" name="txtDescription" id="txtDescription" class="form-control required">
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="control-label col-sm-4">TDS Limit<span style="color:red"> *</span></label>
                            <div class="col-sm-8">
                                <input type="text" tabindex="4" placeholder="Enter TDS Limit" name="txtLimit" id="txtLimit" class="decimal form-control required">
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="control-label col-sm-4">TDS Signle Trans. Limit<span style="color:red"> *</span></label>
                            <div class="col-sm-8">
                                <input type="text" tabindex="5" placeholder="Enter TDS Signle Transaction Limit" name="txtSingleTransLimit" id="txtSingleTransLimit" class="decimal form-control required">
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="control-label col-sm-4">Payable Account</label>
                            <div class="col-sm-8">
                                <input type="text" class="form-control" name="txtPayableAccount" id="txtPayableAccount" tabindex="6" onkeypress="AutosuggestAccountName(this)" placeholder="Enter Payable Account" style="text-transform: uppercase;">
                                @*<select type="text" tabindex="6" name="txtPayableAccount" id="txtPayableAccount" class="form-control">
                                        <option value="" selected>---Select Account---</option>
                                        <option value="TDS ON INTEREST">TDS ON INTEREST</option>
                                        <option value="TDS ON ADVERTISEMENT">TDS ON ADVERTISEMENT</option>
                                        <option value="TDS ON LABOUR">TDS ON LABOUR</option>
                                        <option value="TDS ON SECURITIES">TDS ON SECURITIES</option>
                                        <option value="TDS ON 194H-94H">TDS ON 194H-94H</option>
                                        <option value="TDS ON RENT">TDS ON RENT</option>
                                        <option value="TDS ON 194J-94J (PROFESSIONAL FEES)">TDS ON 194J-94J (PROFESSIONAL FEES)</option>
                                        <option value="TDS ON 194H-94H (COMMISSION OR BROKERAGE)">TDS ON 194H-94H (COMMISSION OR BROKERAGE)</option>
                                        <option value="TDS PAYABLE (194Q - PURCHASE)">TDS PAYABLE (194Q - PURCHASE)</option>
                                    </select>*@
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="control-label col-sm-4">Receivable Account</label>
                            <div class="col-sm-8">
                                <input type="text" class="form-control" name="txtReceivableAccount" id="txtReceivableAccount" tabindex="7" onkeypress="AutosuggestAccountName(this)" placeholder="Enter Receivable Account" style="text-transform: uppercase;">
                                @*<select type="text" tabindex="7" name="txtReceivableAccount" id="txtReceivableAccount" class="form-control">
                                        <option value="" selected>---Select Account---</option>
                                        <option value="TDS ON INTEREST">TDS ON INTEREST</option>
                                        <option value="TDS ON ADVERTISEMENT">TDS ON ADVERTISEMENT</option>
                                        <option value="TDS ON LABOUR">TDS ON LABOUR</option>
                                        <option value="TDS ON SECURITIES">TDS ON SECURITIES</option>
                                        <option value="TDS ON 194H-94H">TDS ON 194H-94H</option>
                                        <option value="TDS ON RENT">TDS ON RENT</option>
                                        <option value="TDS ON 194J-94J (PROFESSIONAL FEES)">TDS ON 194J-94J (PROFESSIONAL FEES)</option>
                                        <option value="TDS ON 194H-94H (COMMISSION OR BROKERAGE)">TDS ON 194H-94H (COMMISSION OR BROKERAGE)</option>
                                        <option value="TDS PAYABLE (194Q - PURCHASE)">TDS PAYABLE (194Q - PURCHASE)</option>
                                    </select>*@
                            </div>
                        </div>

                        @*<div class="form-group">
                                <label class="control-label col-sm-4">Account Name</label>
                                <div class="col-sm-8">
                                    <input type="text" class="form-control" name="txtAccountName" id="txtAccountName">
                                </div>
                            </div>*@

                        <div class="form-group">
                            <label class="control-label col-sm-4">Balance Sheet Group</label>
                            <div class="col-sm-8">
                                <select type="text" class="form-control" name="ddlBalnceSheetGroup" id="ddlBalnceSheetGroup"></select>
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="control-label col-sm-4 txtR">Is Active</label>
                            <div class="col-sm-8">
                                <input type="checkbox" tabindex="8" name="txtIsActive" checked class="icheckminimal" />
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="control-label col-sm-4 txtR">Is TDS Trading</label>
                            <div class="col-sm-8">
                                <input type="checkbox" tabindex="9" name="txtIsTDSTrading" checked class="icheckminimal" />
                            </div>
                        </div>

                    </form>
                </div>
                <div class="modal-footer">
                    @if (Model.ISADD == 1 || Model.ISUPDATE == 1)
                    {
                        <button type="button" id="btnSaveTDSMaster" class="btn btn-success btnSave" tabindex="9"><span class="fa fa-floppy-o"></span> Save</button>
                    }
                    <button name="CancelTDSMaster" class="btn bg-slate-400 btnCancel" type="button"><i class="fa fa-times"></i> Cancel</button>
                </div>

            </div>
        </div>
    </div>
    <!-- Add/Edit form modal -->
    <!-- Delete form modal -->
    <div id="panelTDSMasterDelete" class="modal fade" data-backdrop="static" data-keyboard="false">
        <div class="modal-dialog modal-sm">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title">TDS Master</h5>
                </div>
                <form class="form-horizontal" id="frmCommon_CountryMaster" method="get">
                    <div class="modal-body">
                        <div class="form-group">
                            <dl class="dl-horizontal">
                                <dt>TDS Code: </dt>
                                <dd id="delblTDSCode"></dd>
                            </dl>
                        </div>
                    </div>
                    <div class="modal-footer">
                        @if (Model.ISDELETE == 1)
                        {
                            <button type="button" id="btnDeleteTDSMaster" class="btn btn-danger btnDelete"><span class="fa fa-trash-o"></span> Delete</button>
                        }
                        <button name="CancelTDSMaster" class="btn bg-slate-400 btnCancel" type="button"><i class="fa fa-times"></i> Cancel</button>
                    </div>
                </form>
            </div>
        </div>
    </div>
    <!-- /Delete form modal -->

    <div class="panel" id="panelTDSMasterList" style="display: block; margin-bottom: 0px;">
        <div class="row panel-heading">
            <div class="col-lg-7 pt-5 pl-0">
                <div class="clearfix">
                    <div class="caption">
                        <i class="fa fa-th-list"></i>
                        <span class="caption-subject font-common-sharp bold">TDS Master</span>
                    </div>
                </div>
            </div>
            <div class="col-lg-5 pr-0">
                <div class="col-sm-8 txtR">
                    <div class="form-group has-feedback">
                        <input type="text" id="txtsearchbox" class="form-control searchbox" placeholder="Search Here..." autofocus>
                        <div class="form-control-feedback">
                            <i class="icon-search4 text-size-base"></i>
                        </div>
                    </div>
                </div>
                <div class="col-sm-4 pr-0 txtR">
                    @if (Model.ISADD == 1)
                    {
                        <button class="formstylebutton  btn btn-success waves-effect waves-light m-t-10 btnAddNew" type="button" id="btnAddTDSMaster" style="font-weight:600!important"><i class="fa fa-plus"></i>&nbsp;&nbsp;Add New</button>
                    }
                    <button class="formstylebutton ExitWindow divflotR btn alpha-orange text-orange-800 border-orange-600 ml-5 pull-right" type="button"><i class="fa fa-close"></i>&nbsp;&nbsp;Exit</button>
                </div>
            </div>
        </div>
        <div class="panel-body" style="padding: 0px 10px 18px 7px !important">
            <div class="col-sm-12" style="overflow-x:auto; overflow-y:hidden; ">
                <div class="jqGrid_wrapper" id="jqGrid_TDSMaster">
                    <table id="table_TDSMaster"></table>
                    <div id="pager_TDSMaster"></div>
                </div>
            </div>
        </div>
    </div>
    <input type="hidden" id="hdnTDSId" value="" />
</div>

@section scripts{
    <script src="~/Scripts/MasterScripts/Master/TDSMaster.js?v=@DateTime.UtcNow"></script>
    @*<script type="text/x-jsrender" id="ddlPartyName">
            <option value="{{>accountid}}">{{>accountname}}</option>
        </script>*@
    <script id="BalnceSheetGroupList" type="text/x-jsrender">
        <option value="{{>balancesheetgroupid}}">{{>bsgroupname.replaceAll('&amp;','&')}}</option>
    </script>
}





