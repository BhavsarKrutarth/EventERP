@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="page-container">
    <!-- Add/Edit form modal -->
    <div id="panelAccountYearMasterEdit" class="modal fade" data-backdrop="static" data-keyboard="false">
        <div class="modal-dialog modal-md">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title">Account Year Master</h5>
                </div>
                <div class="modal-body">
                    <form class="form-horizontal" id="frmAccountYearMaster" method="get">
                        <div class="form-group">
                            <label class="control-label col-sm-4 txtR">Account Year Name<span style="color:red"> *</span></label>
                            <div class="col-sm-8">
                                <input type="text" class="form-control" placeholder="Account Year Name" tabindex="1" readonly="readonly" name="txtAccountYearName" id="txtAccountYearName" autofocus>
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="control-label col-sm-4 txtR">Start Date<span style="color:red"> *</span></label>
                            <div class="col-sm-4">
                                <select name="StartMonth" id="ddlStartMonth" tabindex="2" class="form-control required">

                                    <option value="01">January</option>
                                    <option value="02">Febuary</option>
                                    <option value="03">March</option>
                                    <option value="04" selected="selected">April</option>
                                    <option value="05">May</option>
                                    <option value="06">June</option>
                                    <option value="07">July</option>
                                    <option value="08">August</option>
                                    <option value="09">September</option>
                                    <option value="10">October</option>
                                    <option value="11">November</option>
                                    <option value="12">December</option>
                                </select>
                            </div>
                            <div class="col-sm-4">
                                <select name="StartYear" id="ddlStartYear" tabindex="3" class="form-control required">
                                    <option value="">Year</option>
                                    <option value="2015">2015</option>
                                    <option value="2016">2016</option>
                                    <option value="2017">2017</option>
                                    <option value="2018">2018</option>
                                    <option value="2019">2019</option>
                                    <option value="2020">2020</option>
                                    <option value="2021">2021</option>
                                    <option value="2022">2022</option>
                                    <option value="2023">2023</option>
                                    <option value="2024">2024</option>
                                    <option value="2025">2025</option>
                                    <option value="2026">2026</option>
                                    <option value="2027">2027</option>
                                    <option value="2028">2028</option>
                                    <option value="2029">2029</option>
                                    <option value="2030">2030</option>
                                    <option value="2031">2031</option>
                                    <option value="2032">2032</option>
                                    <option value="2033">2033</option>
                                    <option value="2034">2034</option>
                                    <option value="2035">2035</option>
                                    <option value="2036">2036</option>
                                    <option value="2037">2037</option>
                                    <option value="2038">2038</option>
                                    <option value="2039">2039</option>
                                    <option value="2040">2040</option>

                                </select>
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="control-label col-sm-4 txtR">End Date<span style="color:red"> *</span></label>
                            <div class="col-sm-4">
                                <select name="EndMonth" id="ddlEndMonth" tabindex="4" class="form-control required">

                                    <option value="01">January</option>
                                    <option value="02">Febuary</option>
                                    <option value="03" selected>March</option>
                                    <option value="04">April</option>
                                    <option value="05">May</option>
                                    <option value="06">June</option>
                                    <option value="07">July</option>
                                    <option value="08">August</option>
                                    <option value="09">September</option>
                                    <option value="10">October</option>
                                    <option value="11">November</option>
                                    <option value="12">December</option>
                                </select>
                            </div>
                            <div class="col-sm-4">
                                <select name="EndYear" id="ddlEndYear" tabindex="5" class="form-control required">
                                    <option value="">Year</option>
                                    <option value="2015">2015</option>
                                    <option value="2016">2016</option>
                                    <option value="2017">2017</option>
                                    <option value="2018">2018</option>
                                    <option value="2019">2019</option>
                                    <option value="2020">2020</option>
                                    <option value="2021">2021</option>
                                    <option value="2022">2022</option>
                                    <option value="2023">2023</option>
                                    <option value="2024">2024</option>
                                    <option value="2025">2025</option>
                                    <option value="2026">2026</option>
                                    <option value="2027">2027</option>
                                    <option value="2028">2028</option>
                                    <option value="2029">2029</option>
                                    <option value="2030">2030</option>
                                    <option value="2031">2031</option>
                                    <option value="2032">2032</option>
                                    <option value="2033">2033</option>
                                    <option value="2034">2034</option>
                                    <option value="2035">2035</option>
                                    <option value="2036">2036</option>
                                    <option value="2037">2037</option>
                                    <option value="2038">2038</option>
                                    <option value="2039">2039</option>
                                    <option value="2040">2040</option>
                                </select>
                            </div>
                        </div>

                        <input type="text" id="hdnAccountYearId" hidden />
                    </form>
                </div>
                <div class="modal-footer">
                    @if (Model.ISADD == 1 || Model.ISUPDATE == 1)
                    {
                        <button type="button" id="btnSaveAccountYearMaster" class="btn btn-success btnSave" tabindex="6"><span class="fa fa-floppy-o"></span> Save</button>
                    }
                    <button name="CancelAccountYearMaster" class="btn bg-slate-400 btnCancel" type="button" tabindex="7"><i class="fa fa-times"></i> Cancel</button>
                </div>

            </div>
        </div>
    </div>
    <!-- Add/Edit form modal -->
    <!-- Delete form modal -->
    <div id="panelAccountYearMasterDelete" class="modal fade" data-backdrop="static" data-keyboard="false">
        <div class="modal-dialog modal-sm">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title">Account Year Master</h5>
                </div>
                <form class="form-horizontal" id="frmCommon_AccountYearMaster" method="get">
                    <div class="modal-body">
                        <div class="form-group">
                            <dl class="dl-horizontal">
                                <dt>Account Year Name: </dt>
                                <dd id="delblAccountYearName"></dd>
                            </dl>
                        </div>
                    </div>
                    <div class="modal-footer">
                        @if (Model.ISDELETE == 1)
                        {
                            <button type="button" id="btnDeleteAccountYearMaster" class="btn btn-danger btnDelete"><span class="fa fa-trash-o"></span> Delete</button>
                        }
                        <button name="CancelAccountYearMaster" class="btn bg-slate-400 btnCancel" type="button"><i class="fa fa-times"></i> Cancel</button>
                    </div>
                </form>
            </div>
        </div>
    </div>
    <!-- /Delete form modal -->

    <div class="panel" id="panelAccountYearMasterList" style="display: block; margin-bottom: 0px;">
        <div class="row panel-heading pb-0">
            <div class="col-md-10 col-sm-6 pt-10 pl-0">
                <div class="clearfix">

                    <div class="caption">
                        <i class="fa fa-th-list"></i>
                        <span class="caption-subject font-common-sharp bold">Account Year Master</span>
                    </div>
                </div>
            </div>
            @if (Model.ISADD == 1)
            {
                <div class="col-md-2 No-m-display pr-0" style="padding-left: 14px;">
                    <div class="mt7 clearfix pull-right">
                        <button type="button" class="formstylebutton btn btn-success waves-effect waves-light m-t-10 btnAddNew" id="btnAddAccountYearMaster"><b><i class="fa fa-plus"></i></b> Add New</button>
                        <button class="formstylebutton ExitWindow divflotR btn alpha-orange text-orange-800 border-orange-600 ml-5 pull-right" type="button"><i class="fa fa-close"></i>&nbsp;&nbsp;Exit</button>
                    </div>
                </div>
            }
        </div>
        <div class="panel-body pt-0">
            <div class="" style="margin-top: 10px;">
                <div class="jqGrid_wrapper" id="jqgrid_AccountYearMaster">
                    <table id="table_AccountYearMaster"></table>
                    <div id="pager_AccountYearMaster"></div>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- /page container -->
@section Scripts{
    <script src="~/Scripts/MasterScripts/Master/AccountYearMaster.js?v=@DateTime.UtcNow"></script>
}

